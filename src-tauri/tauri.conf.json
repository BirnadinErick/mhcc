{
	"build": {
		"beforeBuildCommand": "pnpm build",
		"beforeDevCommand": "pnpm dev",
		"devPath": "http://localhost:2003",
		"distDir": "../build"
	},
	"package": {
		"productName": "mhcc",
		"version": "0.2.0"
	},
	"tauri": {
		"allowlist": {
			"all": false
		},
		"bundle": {
			"active": true,
			"category": "ECommerce",
			"copyright": "All Rights Reserved by Birnadin Erick 2022",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "mhcc.birnadine.guru",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "",
			"targets": ["msi", "updater"],
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": "Content-Security-Policy: default-src 'self'; img-src 'self'"
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"fullscreen": false,
				"height": 720,
				"resizable": true,
				"title": "Mathagal Health Care Center",
				"width": 1280
			}
		]
	}
}
